{
  "version": 2,
  "name": "dhavalveera",
  "alias": "dhavalveera.macrocephal.cloud",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "index.js",
      "config": {
        "maxLambdaSize": "50mb"
      },
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": ".*",
      "dest": "index.js"
    }
  ]
}
